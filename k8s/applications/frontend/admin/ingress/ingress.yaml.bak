apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: frontend-admin-ingress
  labels:
    env: production
    tier: frontend
    app: admin
    name: frontend-admin-ingress
spec:
  rules:
    - host: admin.localhost
      http:
        paths:
          - path: /
            backend:
              serviceName: frontend-admin
              servicePort: 3000